{
    "name": "winter_24_hackathon",
    "version": "1.0.0",
    "description": "An app for tracking various metrics impacting employees' sleep and thus workplace performance.",
    "scripts": {
        "build": "npm -prefix frontend run build",
        "preinstall": "cd ./backend && npm install",
        "install": "cd ./frontend && npm install",
        "start": "npx concurrently 'npm run start:backend' 'npm run start:frontend'",
        "start:backend": "npm --prefix ./backend start",
        "start:frontend": "npm --prefix ./frontend start",
        "start:dev": "npx concurrently \"npm run start:dev --prefix ./backend\" \"npm start --prefix ./frontend\" ",
        "test": "npm run test:frontend && npm run test:backend",
        "test:backend": "npm test --prefix ./backend",
        "test:frontend": "npm test --prefix ./frontend"
    },
    "repository": {
        "type": "git",
        "url": "https://www.github.com/engineerpehr/winter_24_hackathon.git"
